              "url": {
                "raw": "{{baseUrl}}/agendamentos?cliente={{clienteId}}&status=agendado",
                "host": ["{{baseUrl}}"],
                "path": ["agendamentos"],
                "query": [
                  {
                    "key": "cliente",
                    "value": "{{clienteId}}",
                    "description": "Opcional: Filtrar por cliente"
                  },
